{
  "name": "Cuteblock plus+ for memehub.sk",
  "description": "Plugin to blocking pitkes's dailycutes",
  "version": "1.3",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://memehub.sk/*",
        "http://www.memehub.sk/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ]
}